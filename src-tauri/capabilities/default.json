{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": [
      "http://127.0.0.1:*/",
      "http://*",
      "https://*",
      "ipc://*",
      "socks5://*",
      "*://*"
    ]
  },
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:default",
    "fs:scope-data",
    "fs:scope-data-index",
    "fs:allow-app-meta",
    "fs:allow-app-read",
    "fs:allow-app-write",
    "fs:allow-exists",
    "fs:scope-app",
    "fs:allow-data-read-recursive",
    "fs:allow-data-write-recursive",
    "fs:allow-data-read",
    "fs:allow-data-write",
    "fs:allow-data-meta-recursive",
    "fs:allow-data-meta",
    "fs:scope-data-recursive",
    "os:default"
  ]
}